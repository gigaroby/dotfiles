ZSH=$HOME/.oh-my-zsh
ZSH_THEME="gentoo"

plugins=(git git-flow django pip python history-substring-search brew)

source $ZSH/oh-my-zsh.sh
export PATH=$HOME/.gradle-bins/bin:$PATH
export PATH=$HOME/projects/go/bin:/usr/local/bin:$HOME/.goproj/bin:$PATH
export PATH=$PATH:$HOME/.mvim
export PATH=$PATH:/Applications/Postgres93.app/Contents/MacOS/bin

export HOMEBREW_MAKE_JOBS=8

source "/usr/local/bin/virtualenvwrapper.sh"
alias go="goproj-go"
alias mvim="goproj mvim"
alias godoc="goproj godoc"
alias pull="git pull --rebase"

export EDITOR="vim"

alias cname="dig +short CNAME"
alias a="dig +short A"
